{
    "name": "zhuger/webayun-upgradeengine",
    "description": "upgrade package for php",
    "keywords": [
        "webayun"
    ],
    "type": "library",
    "authors": [
        {
            "name": "zhuger",
            "email": "543585543@qq.com"
        }
    ],
    "license": "Apache-2.0",
    "require": {
        "php": ">=5.6"
    },
    "minimum-stability": "dev-master",
    "autoload": {
        "psr-4": {
            "webayun\\upgradeengine\\": "src"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "extra": {
        "think": {
            "config":{
                "upgrade": "src/config.php"
            }
        }
    }
}
